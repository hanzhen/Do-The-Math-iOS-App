#import "TwentyFour+Functions.h"
#import "Round.h"
@implementation TwentyFour (Functions)

/**
 * Gives a description of the 24 object
 */

-(NSString *)description {
    NSString *description = [NSString stringWithFormat:@"Question:[id:%@,typeround:%@,number1:%@, number2:%@, number3:%@, number4:%@, solution:%@, yourtimeSpent: %d, theirtimeSpent: %d] ",self.questionId,self.round.type ,self.number1,self.number2,self.number3,self.number4, self.solution, [self.yourTimeSpent intValue], [self.theirTimeSpent intValue]];
    return description;
    
}

@end
